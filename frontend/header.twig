    <script src="https://cdn.datatables.net/scroller/2.0.5/js/dataTables.scroller.min.js" defer></script>  

    <!-- Leaflet JS -->
    <script src="https://unpkg.com/leaflet/dist/leaflet.js" defer></script>
    <script src="https://cdn.jsdelivr.net/npm/leaflet-polylinedecorator" defer></script>

    <!-- Google Tag Manager -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-L2SMMG4DKT"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());
        gtag('config', 'G-L2SMMG4DKT');
    </script>

    <script>
        // Atualiza a página a cada 120.000 milissegundos (2 minutos) em páginas válidas
        const currentUrl = window.location.pathname;
        const validPages = ['/home', '/dashboard', '/routes', '/irregularidades'];

        if (validPages.includes(currentUrl)) {
            setInterval(function() {
                window.location.reload();
            }, 120000); // Recarregar página a cada 2 minutos
        }
    </script>
</head>
